CREATE TABLE PRODUCT_MASTER
(
 PRODUCT_NO VARCHAR2(6),
 DESCRIPTION VARCHAR2(15),
 PROFIT_PERCENTAGE NUMBER(4,2),
 UNIT_MEASURE VARCHAR2(10),
 QTY_ON_HAND NUMBER(8),
 REORDER_LVL NUMBER(8,2),
 SELL_PRICE NUMBER(8,2),
 COST_PRICE NUMBER(8,2)
);
DESC PRODUCT_MASTER;

DROP TABLE PRODUCT_MASTER;

SELECT *FROM PRODUCT_MASTER;

SELECT DESCRIPTION FROM PRODUCT_MASTER;

INSERT INTO PRODUCT_MASTER(PRODUCT_NO,DESCRIPTION,PROFIT_PERCENTAGE,UNIT_MEASURE,QTY_ON_HAND,REORDER_LVL,SELL_PRICE,COST_PRICE) VALUES('P07975','1.22 DRIVE',5,'PIECE',75,30,450,300);


CREATE TABLE CLIENT_MASTER(

CLIENT_NO VARCHAR2(6) PRIMARY KEY,

NAME VARCHAR2(20) NOT NULL,

ADDRESS VARCHAR2(30),

CITY VARCHAR2(15),

PINCODE NUMBER(6),

STATE VARCHAR2(20),

BAL_DUE NUMBER(8,2)
);
DESC CLIENT_MASTER;

INSERT INTO CLIENT_MASTER(CLIENT_NO,NAME,ADDRESS,CITY,PINCODE,STATE,BAL_DUE) VALUES('C00006','NISHANT TYAGI','B-20','BADAGAON',560050,'BAGHPAT UTTARPRADESH',2000);

